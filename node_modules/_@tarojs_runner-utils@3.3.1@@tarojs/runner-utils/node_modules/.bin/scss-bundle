#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_scss-bundle@3.1.2@scss-bundle/dist/cli/main.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_scss-bundle@3.1.2@scss-bundle/dist/cli/main.js" "$@"
  ret=$?
fi
exit $ret
